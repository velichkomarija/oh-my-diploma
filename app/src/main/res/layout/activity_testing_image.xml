<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_marginVertical="30dp"
    android:layout_height="match_parent"
    tools:context="com.example.velickomarija.diploma.TestingImageActivity">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:gravity="center"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="703dp"
        tools:layout_editor_absoluteY="7dp">

        <TableLayout
            android:id="@+id/tableLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp">

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol1"

                    tools:layout_editor_absoluteX="50dp"
                    tools:layout_editor_absoluteY="50dp" />

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol2"
                    tools:layout_editor_absoluteX="160dp"
                    tools:layout_editor_absoluteY="50dp" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol3"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="50dp" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol4"
                    tools:layout_editor_absoluteX="380dp"
                    tools:layout_editor_absoluteY="50dp" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol5"
                    tools:layout_editor_absoluteX="490dp"
                    tools:layout_editor_absoluteY="50dp" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol6"
                    tools:layout_editor_absoluteX="600dp"
                    tools:layout_editor_absoluteY="50dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp">

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol7"
                    tools:layout_editor_absoluteX="50dp"
                    tools:layout_editor_absoluteY="175dp" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol9"
                    tools:layout_editor_absoluteX="160dp"
                    tools:layout_editor_absoluteY="175dp" />

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol8"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="175dp" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol10"
                    tools:layout_editor_absoluteX="380dp"
                    tools:layout_editor_absoluteY="175dp" />

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol11"
                    tools:layout_editor_absoluteX="490dp"
                    tools:layout_editor_absoluteY="175dp" />

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/symbol12"
                    tools:layout_editor_absoluteX="600dp"
                    tools:layout_editor_absoluteY="175dp" />
            </TableRow>

        </TableLayout>

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="50dp"
            android:layout_height="120dp"
            android:layout_marginVertical="50dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/ic_timer_black_24dp"
            tools:layout_editor_absoluteX="774dp"
            tools:layout_editor_absoluteY="49dp" />

        <TextView
            android:id="@+id/textViewTimer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:text="@string/timer"
            android:textColor="@color/black"
            android:textSize="35sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="774dp"
            tools:layout_editor_absoluteY="219dp" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
